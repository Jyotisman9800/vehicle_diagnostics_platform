{
  "version": 2,
  "name": "oem-diagnostics",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)\\.(css|js|jsx|png|jpg|svg|ico|woff|woff2)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
  /* Note: No rewrites required since we use hash routing (#/...).
     Rewrites here would otherwise intercept /styles.css and /app.jsx. */
}
